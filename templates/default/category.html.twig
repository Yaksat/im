{% extends 'base.html.twig' %}

{% block title %}{{ category.name }}{% endblock %}

{% block body %}
    <h1>{{ category.name }}</h1>
    <ul>
        {% for product in products %}
            <li><a href="{{ path('product', {id: product.id}) }}">{{ product.name }}</a> </li>
        {% endfor %}
    </ul>
{% endblock %}